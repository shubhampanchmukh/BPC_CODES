void draw()
{if (frame==0)
   u8g.drawBitmapP( 0, 0, 16, 64, Bpc);
 else if(frame==1)
   u8g.drawBitmapP( 0, 0, 16, 64, battery_1);
 else if(frame==2)
   u8g.drawBitmapP( 0, 0, 16, 64, battery_2);
 else if(frame==3)
   u8g.drawBitmapP( 0, 0, 16, 64, battery_3);
 else
   u8g.drawBitmapP( 0, 0, 16, 64, battery_4);
}


void setup(void) {
}

void loop(void) {

 u8g.firstPage();  
  do {
    draw();
    
  } while( u8g.nextPage() );
  if(frame==0)
      delay(10000);
    else
      delay(3000);
   frame++;
   if(frame>4)
   frame=0;
   
   
 

}
